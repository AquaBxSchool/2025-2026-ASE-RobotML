{
	"name": "robot-ml-cli",
	"description": "The cli specific package",
	"version": "0.0.1",
	"type": "module",
	"engines": {
		"node": ">=20.10.0",
		"npm": ">=10.2.3"
	},
	"files": [
		"dist",
		"src"
	],
	"bin": {
		"robot-ml-cli": "./dist/cli.js"
	},
	"scripts": {
		"clean": "shx rm -fr *.tsbuildinfo dist",
		"build": "rolldown -c rolldown.config.ts && bun build ./dist/main.js --compile --outfile=./dist/cli",
		"test": "bun ./src/ast.ts"
	},
	"devDependencies": {
		"robot-ml-language": "*",
		"chalk": "~5.6.2",
		"commander": "~14.0.2",
		"@types/bun": "^1.3.7"
	},
	"volta": {
		"node": "20.19.2",
		"npm": "10.8.2"
	}
}
